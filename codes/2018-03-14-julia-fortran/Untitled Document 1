module julia_arr
    implicit none
    contains

    subroutine bar(x, a, b)
        integer, intent(in) :: x
        integer, intent(out) :: a, b

        a = x + 3
        b = x * 3
    end subroutine bar

    subroutine mul2(x, y) bind (c)
        real, dimension(3), intent(in) :: x
        real, dimension(3), intent(out) :: y
        integer :: i
        do i=1,3
            y(i) = 2*x(i)
        end do
    end subroutine mul2


end module julia_arr

